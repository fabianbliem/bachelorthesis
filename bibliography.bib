journal = {Transportation Research Interdisciplinary Perspectives},
keywords = {COVID-19,Developing country,Global pandemic,Lockdown,Travel behaviour},
mendeley-groups = {Bacheloarbeit},
number = {November 2020},
title = {{Exploring the travel behavior changes caused by the COVID-19 crisis: A case study for a developing country}},
